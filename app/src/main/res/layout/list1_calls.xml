<?xml version="1.0" encoding="utf-8"?>
<androidx.cardview.widget.CardView xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_margin="@dimen/_5sdp"
    app:cardCornerRadius="@dimen/_10sdp"
    app:cardElevation="@dimen/_5sdp">

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:orientation="horizontal"
        android:layout_margin="@dimen/_5sdp"
        >
     <LinearLayout
         android:layout_width="@dimen/_265sdp"
         android:layout_height="wrap_content"
         android:orientation="vertical"
         >
         <LinearLayout
             android:layout_width="wrap_content"
             android:layout_height="wrap_content"
             android:orientation="horizontal"
             android:layout_margin="@dimen/_5sdp"
             >

             <ImageView
                 android:layout_width="@dimen/_15sdp"
                 android:layout_height="@dimen/_15sdp"
                 android:src="@drawable/ic_group_2878" />

             <TextView
                 android:id="@+id/name_list1_calls"
                 android:layout_width="wrap_content"
                 android:layout_height="wrap_content"
                 android:textSize="@dimen/_9ssp"
                 android:fontFamily="@font/poppins_regular"

                 android:paddingStart="@dimen/_5sdp"
                 android:textColor="@color/c15"
                 android:textAlignment="viewStart" />

         </LinearLayout>
         <LinearLayout
             android:layout_width="wrap_content"
             android:layout_height="wrap_content"
             android:orientation="horizontal"
             android:layout_margin="@dimen/_5sdp"
             >

             <ImageView
                 android:layout_width="@dimen/_15sdp"
                 android:layout_height="@dimen/_15sdp"
                 android:src="@drawable/ic_group_2877" />

             <TextView
                 android:id="@+id/date_list1_calls"
                 android:layout_width="wrap_content"
                 android:layout_height="wrap_content"
                 android:textSize="@dimen/_9ssp"
                 android:fontFamily="@font/poppins_regular"

                 android:paddingStart="@dimen/_5sdp"
                 android:textColor="@color/c15"
                 android:textAlignment="viewStart" />

         </LinearLayout>


     </LinearLayout>

        <ImageView
            android:id="@+id/image_list1_calls"
            android:layout_width="@dimen/_15sdp"
            android:layout_height="@dimen/_15sdp"
            android:src="@drawable/ic_group_4464__1_"
            android:layout_marginTop="@dimen/_5sdp"

            />
    </LinearLayout>


</androidx.cardview.widget.CardView>